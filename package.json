{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node --watch backend/src/server.js",
    "build": "tsc && vite build",
    "preview": "vite preview",
